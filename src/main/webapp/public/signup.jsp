        <h1 class="h3 mb-3 font-weight-normal">Reading Rover</h1>
        <p>Making reading fun for kids!</p>
      </div>
        
      <div class="form-group">
          <label for="username">Username</label>
          <input type="text" id="username" name="username" class="form-control" aria-describedby="usernameHelp" placeholder="Please create a username" v-model.trim="user.username" required autofocus>
          <small id="usernameHelp" class="form-text text-muted">Be creative!</small>
      </div>
      <div class="form-row">
          <div class="form-group col-md-8">
              <label for="f-name">First Name</label>
              <input type="text" id="f-name" name="f-name" class="form-control" placeholder="First Name" v-model.trim="user.first_name"required>
          </div>
          <div class="form-group col-md-4">
              <label for="l-name">Last Initial</label>
              <input type="text" id="l-name" name="l-name" class="form-control" placeholder="I." v-model.trim="user.last_initial" required>
          </div>
      </div>
      <div class="form-group">
          <label for="bday">Birthday</label>
          <input type="date" id="bday" name="bday" class="form-control" v-model.trim="raw_bday_value" required>
      </div>
      <div class="form-group">
          <label for="password">Password</label>
          <input type="password" id="password" name="password" class="form-control" placeholder="Password" autocomplete="new-password" v-model.trim="user.password" required>
      </div>
      <div class="form-group">
          <label for="confirm_password">Confirm Password</label>
          <input type="password" id="confirm_password" name="confirm_password" class="form-control" placeholder="Password" v-model.trim="user.confirm_password" ref="confirm_password_el" required>
      </div>
      <!-- TODO Manage avatar via Vue -->
      <div class="form-group">
          <label for="avatarSelect">Avatar</label>
          <select class="custom-select" id="avatarSelect" onchange="document.getElementById('top-image').setAttribute('xlink:href',this.value)">
              <option value="#SVGReadingRoverLogo" selected>Choose...</option>
              <option value="#Raccoon">Raccoon</option>
              <option value="#Lamb">Lamb</option>
              <option value="#Pig">Pig</option>
              <option value="#Beaver">Beaver</option>
          </select>
      </div>
      
      <button class="btn btn-lg btn-primary btn-block mt-4" v-on:click="signup">Submit</button>
      <p class="mt-5 mb-3 text-muted text-center">&copy; 2018</p>
    </form>

    <script>
        const app = new Vue(
        {
            el: '#app',
            data:
            {
                // Used to convert between jimmutable Day and HTML date picker
                // mm/dd/yyyy
                raw_bday_value: new Date().toISOString().slice(0,10), // Today
                user:
                {
                    username: '',
                    first_name: '',
                    last_initial: '',
                    birthday: '',
                    password: '',
                    confirm_password: '',
                    avatar_id: ''
                }
            },
            computed:
            {
                passwords_match()
                {
                    // Don't nag the user until they've started entering the confirm password
                    if (! this.user.password) return true;
                    if (! this.user.confirm_password) return true;

                    return this.user.password == this.user.confirm_password;
                }
            },
            watch:
            {
                raw_bday_value(new_val, old_val)
                {
                    this.user.birthday = this.convertHtmlToDay(new_val);
                },
                passwords_match(new_val, old_val)
                {
                    if (new_val)
                    {
                        app.$refs.confirm_password_el.setCustomValidity("");
                    }
                    else
                    {
                        app.$refs.confirm_password_el.setCustomValidity("Passwords Must Match");
                    }
                },
            },
            methods:
            {
                signup: function(event)
                {
                    if (! event.target.form.checkValidity())
                    {
                        return;
                    }

                    event.preventDefault();

                    console.log(this.user);

                    axios.post('/public/users/do-signup',
                    {
                        username: this.user.username,
                        first_name: this.user.first_name,
                        last_initial: this.user.last_initial,
                        birthday: this.user.birthday,
                        password: this.user.password,
                        avatar_id: this.user.avatar_id
                    })
                    .then(response =>
                    {
                        console.log("User creation successful");
                        window.location.assign("/home");
                    })
                    .catch(error =>
                    {
                        alert(error.response.data.message);
                    });
                },
                convertDayToHtml: function(date)
                {
                    if (date === "") return date;

                    // Convert mm/dd/yyyy to yyyy-mm-dd
                    let [month, day, year] = date.split("/");
                    month = month.padStart(2, "0");
                    day = day.padStart(2, "0");

                    return `${year}-${month}-${day}`
                },
                convertHtmlToDay: function(date)
                {
                    if (date === "") return date;

                    // Convert yyyy-mm-dd to mm/dd/yyyy
                    let [year, month, day] = date.split("-");
                    return `${month}/${day}/${year}`;
                }
            }
        })
    </script>
  </body>
</html>
